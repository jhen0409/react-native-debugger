{
  "name": "react-native-debugger",
  "version": "0.14.0",
  "productName": "React Native Debugger In Dev",
  "description": "The standalone app for React Native Debugger, with React DevTools / Redux DevTools",
  "main": "electron/main.js",
  "scripts": {
    "pack-macos": "./scripts/package-macos.sh",
    "pack-linux": "./scripts/package-linux.sh",
    "pack-windows": "sh ./scripts/package-windows.sh",
    "pack": "yarn build && ./scripts/package-linux.sh && ./scripts/package-windows.sh && ./scripts/package-macos.sh",
    "start": "cd dist && cross-env NODE_ENV=production PACKAGE=no electron ./",
    "build:main": "cross-env NODE_ENV=production webpack --config webpack/main.prod.js --progress --profile",
    "build:renderer": "cross-env NODE_ENV=production webpack --config webpack/renderer.prod.js --progress --profile",
    "build": "yarn build:main && yarn build:renderer",
    "dev:webpack": "webpack-dev-server --config webpack/renderer.dev.js --port 3000 --allowed-hosts all",
    "dev:electron": "cross-env NODE_ENV=development electron -r @babel/register -r ./electron/debug .",
    "lint": "eslint .",
    "test": "yarn lint && jest __tests__",
    "test-e2e": "jest __e2e__ --runInBand  --forceExit",
    "postinstall": "patch-package && node ./scripts/postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhen0409/react-native-debugger.git"
  },
  "author": "Jhen <developer@jhen.me>",
  "license": "MIT",
  "devDependencies": {
    "@apollo/client": "^3.7.17",
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/plugin-transform-react-constant-elements": "^7.22.5",
    "@babel/plugin-transform-react-inline-elements": "^7.22.5",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/register": "^7.22.5",
    "@electron/notarize": "^2.1.0",
    "@electron/osx-sign": "^1.0.4",
    "@electron/universal": "^1.4.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "cross-env": "^5.2.0",
    "css-loader": "^6.8.1",
    "electron": "^25.3.0",
    "electron-debug": "^3.2.0",
    "electron-installer-dmg": "^4.0.0",
